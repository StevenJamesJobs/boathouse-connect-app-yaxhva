
{
  "cli": {
    "version": ">=16.0.0",
    "appVersionSource": "local"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "autoIncrement": true,
      "cache": {
        "disabled": false
      }
    },
    "preview": {
      "distribution": "internal",
      "autoIncrement": true,
      "cache": {
        "disabled": false
      }
    },
    "production": {
      "autoIncrement": true,
      "cache": {
        "disabled": false
      },
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "buildConfiguration": "Release"
      },
      "env": {
        "EXPO_NO_TELEMETRY": "1"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
