
{
  "cli": {
    "version": ">=16.0.0",
    "appVersionSource": "local"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "autoIncrement": true,
      "cache": {
        "disabled": false
      }
    },
    "preview": {
      "distribution": "internal",
      "autoIncrement": true,
      "cache": {
        "disabled": false
      }
    },
    "production": {
      "autoIncrement": true,
      "cache": {
        "disabled": false
      },
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "buildConfiguration": "Release",
        "credentialsSource": "remote",
        "autoIncrement": true
      },
      "env": {
        "EXPO_NO_TELEMETRY": "1"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "stevenjamesjobs@gmail.com",
        "ascAppId": "6756323107",
        "appleTeamId": "86JRKCMC65"
      },
      "android": {
        "serviceAccountKeyPath": "./google-service-account.json",
        "track": "internal"
      }
    }
  }
}
