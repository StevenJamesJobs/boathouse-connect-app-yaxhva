        "foregroundImage": "./assets/images/57b2b2e5-dd37-4d86-a5d4-03c600dda0ca.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.mcloones.boathouseconnect",
      "versionCode": 12,
      "permissions": [
        "INTERNET",
        "ACCESS_NETWORK_STATE"
      ],
      "softwareKeyboardLayoutMode": "pan"
    },
    "web": {
      "bundler": "metro",
