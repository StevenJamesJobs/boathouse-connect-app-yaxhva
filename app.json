
{
  "expo": {
    "name": "Boathouse Connect",
    "slug": "Boathouse Connect",
    "version": "1.3.0",
    "orientation": "portrait",
    "icon": "./assets/images/57b2b2e5-dd37-4d86-a5d4-03c600dda0ca.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/57b2b2e5-dd37-4d86-a5d4-03c600dda0ca.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mcloones.boathouseconnect",
      "buildNumber": "1.2.4"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/57b2b2e5-dd37-4d86-a5d4-03c600dda0ca.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.mcloones.boathouseconnect",
      "versionCode": 12,
      "permissions": [
        "INTERNET",
        "ACCESS_NETWORK_STATE"
      ],
      "softwareKeyboardLayoutMode": "pan"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/57b2b2e5-dd37-4d86-a5d4-03c600dda0ca.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#ffffff",
          "image": "./assets/images/57b2b2e5-dd37-4d86-a5d4-03c600dda0ca.png",
          "imageWidth": 200
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}