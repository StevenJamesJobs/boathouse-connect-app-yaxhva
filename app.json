      "favicon": "./assets/images/57b2b2e5-dd37-4d86-a5d4-03c600dda0ca.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#ffffff",
          "image": "./assets/images/57b2b2e5-dd37-4d86-a5d4-03c600dda0ca.png",
          "imageWidth": 200
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "f7f98351-f034-4baa-8eb8-ef50cd4aaf7b"
      }
