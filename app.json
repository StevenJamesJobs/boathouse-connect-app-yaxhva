{
  "expo": {
    "name": "Boathouse Connect",
    "slug": "Boathouse Connect",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/43c91958-d4c9-4b12-8d2a-51e85de57f94.jpeg",
    "scheme": "mcloones",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/43c91958-d4c9-4b12-8d2a-51e85de57f94.jpeg",
      "resizeMode": "contain",
      "backgroundColor": "#E8F1F8"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mcloones.boathouseconnect",
      "infoPlist": {
        "UIBackgroundModes": [
          "remote-notification"
        ]
      },
      "icon": "./assets/images/43c91958-d4c9-4b12-8d2a-51e85de57f94.jpeg"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/43c91958-d4c9-4b12-8d2a-51e85de57f94.jpeg",
        "backgroundColor": "#E8F1F8"
      },
      "package": "com.mcloones.boathouseconnect",
      "icon": "./assets/images/43c91958-d4c9-4b12-8d2a-51e85de57f94.jpeg"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/43c91958-d4c9-4b12-8d2a-51e85de57f94.jpeg"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#E8F1F8",
          "image": "./assets/images/43c91958-d4c9-4b12-8d2a-51e85de57f94.jpeg",
          "imageWidth": 280
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  },
  "scheme": "Boathouse Connect"
}