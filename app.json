
{
  "expo": {
    "name": "Boathouse Connect",
    "slug": "boathouse-connect",
    "version": "1.3.0",
    "orientation": "portrait",
    "icon": "./assets/images/57b2b2e5-dd37-4d86-a5d4-03c600dda0ca.png",
    "scheme": "boathouse-connect",
